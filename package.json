{
  "name": "Coinsly",
  "version": "1.0.0",
  "description": "My terrific Nuxt.js project",
  "author": "Damian Mullins",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "build:analyze": "nuxt build --analyze",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "test": "jest"
  },
  "lint-staged": {
    "*.{js,vue}": "yarn lint",
    "*.{css,vue}": "stylelint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@nuxtjs/axios": "5.11.0",
    "@nuxtjs/dotenv": "1.4.1",
    "@nuxtjs/firebase": "6.0.1",
    "@nuxtjs/pwa": "3.0.0-beta.20",
    "firebase": "7.15.1",
    "nuxt": "3.4.3"
  },
  "devDependencies": {
    "@nuxtjs/color-mode": "1.0.0",
    "@nuxtjs/eslint-config": "3.0.0",
    "@nuxtjs/eslint-module": "2.0.0",
    "@nuxtjs/stylelint-module": "4.0.0",
    "@nuxtjs/tailwindcss": "2.0.0",
    "@vue/test-utils": "1.0.3",
    "autoprefixer": "9.8.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.0.1",
    "eslint": "7.2.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-nuxt": "1.0.0",
    "eslint-plugin-prettier": "3.1.4",
    "husky": "4.2.5",
    "jest": "26.0.1",
    "lint-staged": "10.2.11",
    "nuxt-purgecss": "1.0.0",
    "prettier": "2.0.5",
    "stylelint": "13.6.0",
    "vue-jest": "3.0.5"
  },
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  },
  "browserslist": [
    ">= 1% in GB",
    "last 3 Chrome versions",
    "last 2 Firefox versions",
    "last 2 Edge versions",
    "last 3 Safari versions"
  ]
}
